{
    "name": "laravel/laravel",
    "description": "The skeleton application for the Laravel framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "laravel",
        "framework"
    ],
    "require": {
        "php": "^8.4",
        "ext-exif": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "backstage/laravel-og-image": "^0.9.1",
        "based/momentum-trail": "dev-feature/laravel-12",
        "inertiajs/inertia-laravel": "^2.0.1",
        "intervention/image-laravel": "^1.5.5",
        "laravel/framework": "^12.1.1",
        "laravel/sanctum": "^4.0.8",
        "laravel/socialite": "^5.18.0",
        "laravel/tinker": "^2.10.1",
        "predis/predis": "^2.3.0",
        "socialiteproviders/discord": "^4.2.0",
        "socialiteproviders/twitch": "^5.4",
        "spatie/image": "^3.8.0",
        "spatie/laravel-medialibrary": "^11.12.7",
        "spatie/laravel-permission": "^6.16.0",
        "spatie/laravel-query-builder": "^6.3.1",
        "spatie/laravel-sitemap": "^7.3.5",
        "spatie/laravel-sluggable": "^3.7.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "brianium/paratest": "^7.7.0",
        "ergebnis/composer-normalize": "^2.45.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.1.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.21.1",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.6.1",
        "pestphp/pest": "^3.7.4",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-type-coverage": "dev-bugfix/3.3.1",
        "spatie/laravel-ignition": "^2.9.1",
        "spatie/laravel-ray": "^1.40.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ccharz/momentum-trail"
        },
        {
            "type": "git",
            "url": "https://github.com/thimarsola/pest-plugin-type-coverage"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan trail:generate"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:models --dir=\"app/Models/\" -N --write-mixin"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "analyse": "./vendor/bin/phpstan analyse --memory-limit=512M",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "doc:models": "php artisan ide-helper:models --dir=\"app/Models/\" -N --write-mixin",
        "lint": "./vendor/bin/pint",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:type-coverage",
            "@test:unit"
        ],
        "test:arch": "pest --filter=arch",
        "test:lint": "./vendor/bin/pint --test",
        "test:type-coverage": "pest --recreate-databases --type-coverage --min=99 --memory-limit=512M",
        "test:types": "./vendor/bin/phpstan analyse --memory-limit=512M",
        "test:unit": "pest --parallel --recreate-databases --coverage --min=40"
    },
    "$schema": "https://getcomposer.org/schema.json"
}
