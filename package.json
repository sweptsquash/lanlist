{
    "name": "lanlist",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "browserslist": "> 0.2%, last 2 versions, Firefox ESR, not dead",
    "engines": {
        "node": "^22.13.1"
    },
    "scripts": {
        "dev": "run-p routes lint && vite",
        "build:dev": "run-p routes lint && vite build --mode development && vite build --mode development --ssr",
